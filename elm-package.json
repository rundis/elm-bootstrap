{
    "version": "2.0.0",
    "summary": "Elm Bootstrap is a comprehensive library for working with Twitter Bootstrap 4",
    "repository": "https://github.com/rundis/elm-bootstrap.git",
    "license": "BSD3",
    "source-directories": [
        "src/"
    ],
    "exposed-modules": [
        "Bootstrap.Accordion",
        "Bootstrap.Alert",
        "Bootstrap.Badge",
        "Bootstrap.Button",
        "Bootstrap.ButtonGroup",
        "Bootstrap.Card",
        "Bootstrap.CDN",
        "Bootstrap.Dropdown",
        "Bootstrap.Form",
        "Bootstrap.Form.Checkbox",
        "Bootstrap.Form.Input",
        "Bootstrap.Form.InputGroup",
        "Bootstrap.Form.Radio",
        "Bootstrap.Form.Select",
        "Bootstrap.Form.Textarea",
        "Bootstrap.Grid",
        "Bootstrap.Grid.Col",
        "Bootstrap.Grid.Row",
        "Bootstrap.ListGroup",
        "Bootstrap.Modal",
        "Bootstrap.Navbar",
        "Bootstrap.Popover",
        "Bootstrap.Progress",
        "Bootstrap.Tab",
        "Bootstrap.Table",
        "Bootstrap.Text"
    ],
    "dependencies": {
        "debois/elm-dom": "1.0.0 <= v < 2.0.0",
        "elm-lang/animation-frame": "1.0.1 <= v < 2.0.0",
        "elm-lang/core": "5.0.0 <= v < 6.0.0",
        "elm-lang/html": "2.0.0 <= v < 3.0.0",
        "elm-lang/mouse": "1.0.1 <= v < 2.0.0",
        "elm-lang/window": "1.0.1 <= v < 2.0.0"
    },
    "elm-version": "0.18.0 <= v < 0.19.0"
}
